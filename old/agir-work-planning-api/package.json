    "create-dts": "ts-node ./open-api/dtsgenerator.ts",
    "create-mongo-schema": "ts-node ./open-api/mongo-schema-gen.ts",
    "load-data": "node_modules/.bin/gulp load-data --interv 5000",
    "link-lib": "npm link @villemontreal/agir-work-planning-lib",
    "unlink-lib": "npm unlink --no-save @villemontreal/agir-work-planning-lib && npm install",
    "import:internal": "ts-node scripts/import/internal/internal.ts",
    "import:internal:send": "ts-node scripts/import/internal/internal-send.ts",
    "diff-geo-layers": "ts-node ./scripts/diff-geo-layers/start.ts",
    "ci": "npm install && npm run clean && npm run test",
    "sonar": "sh ./sonar.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-fix",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run test-units"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/villemontreal/my-node-api"
