    "test-units": "node_modules/.bin/gulp test-units",
    "test-load": "node_modules/.bin/gulp test-load",
    "compile": "node_modules/.bin/gulp compile",
    "clean": "node_modules/.bin/gulp compile clean && git clean -X -f src/ tests/",
    "debug": "node_modules/.bin/gulp debug",
    "watch": "node_modules/.bin/gulp watch",
    "editor": "node_modules/.bin/gulp editor",
    "validate": "node_modules/.bin/gulp validate",
    "docker": "node .docker/docker-local-debug.js",
    "lint": "node_modules/.bin/gulp lint",
