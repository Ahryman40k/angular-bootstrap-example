    "@turf/bbox-polygon": "6.3.0",
    "@turf/turf": "^5.1.6",
    "@types/bootstrap": "^4.3.0",
    "@types/lodash": "^4.14.144",
    "@types/shapefile": "0.6.0",
    "@villemontreal/agir-work-planning-lib": "0.16.0-pre.build.b1ceed7-58",
