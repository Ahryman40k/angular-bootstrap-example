{
  "name": "agir-work-planning-web",
  "version": "2.8.25",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0",
    "build": "node --max_old_space_size=32000 ./node_modules/@angular/cli/bin/ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:dev": "npm run build -- --configuration=development",
    "build:accept": "npm run build -- --configuration=accept",
    "build:training@accept": "ng build --configuration=training@accept",
    "build:prod": "npm run build -- --configuration=production",
    "test": "ng test  --code-coverage",
    "test:w": "ng test --watch --code-coverage",
    "lint-fix": "ng lint --fix && npm run prettier-fix",
    "prettier-fix": "prettier \"**/*.ts\" --write",
    "start:mock-server": "json-server --host 0.0.0.0 --watch server-mock/json-server.json --routes server-mock/routes.json",
    "ci": "npm install && npm run lint && npm run build:dev",
    "link-lib": "npm link @villemontreal/agir-work-planning-lib",
    "unlink-lib": "npm unlink --no-save @villemontreal/agir-work-planning-lib && npm install",
    "sonar": "sh ./sonar.sh"
  },
  "private": true,
  "dependencies": {
